import java.util.Scanner;

public class Main {	 
	public static void main(String[] args)  {
		try (Scanner sc = new Scanner(System.in)) {
			
			
			int t = sc.nextInt();
			
			for (int i=0; i<t; i++) {
				int n =sc.nextInt();
				String str = sc.next();
				for (int j=0; j<str.length(); j++) {
					for (int k=0; k<n; k++) {
						char c = str.charAt(j);
						System.out.print(c);
					}
				}
			System.out.println();
			}
						
						
			}
		}
	}



