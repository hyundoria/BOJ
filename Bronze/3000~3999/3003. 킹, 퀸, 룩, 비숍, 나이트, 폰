import java.util.*;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int arr[] = new int[6];
		int arr1[] = new int[6];
		
		for(int i=0; i<6; i++) {
			arr[i] = sc.nextInt();
		}
	
		arr1[0] = 1-arr[0];
		arr1[1] = 1-arr[1];
		arr1[2] = 2-arr[2];
		arr1[3] = 2-arr[3];
		arr1[4] = 2-arr[4];
		arr1[5] = 8-arr[5];
		
		for(int i=0; i<6; i++) {
			System.out.printf("%d " ,arr1[i]);
		}
	}

}
