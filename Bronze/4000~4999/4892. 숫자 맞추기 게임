import java.util.Scanner;

public class Main {	 
	public static void main(String[] args)  {
		try (Scanner sc = new Scanner(System.in)) {
			
									
			int count = 0;
			
			while (true) {
				int n_0 = sc.nextInt();
				
				int n_1 = n_0 * 3;
				
				if (n_0 == 0) {
					break;					
				}
				
				if (n_1 % 2 == 0) {
					int n_2 = n_1 / 2;
					int n_3 = n_2 * 3;
					int n_4 = n_3 / 9;
					count ++;
					System.out.println(count + ". " +"even " + n_4);
					
				}
				else if (n_1 % 2 != 0){
					int n_2 = (n_1 + 1) / 2;				
					int n_3 = n_2 * 3;
					int n_4 = n_3 / 9;
					count ++;
					System.out.println(count + ". " +"odd " + n_4);
				}
				
						
			}
			
			
			}
		}
	}

