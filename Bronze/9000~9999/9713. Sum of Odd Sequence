import java.util.Scanner;

public class Main {	 
	public static void main(String[] args)  {
		try (Scanner sc = new Scanner(System.in)) {
			
				
			int n = sc.nextInt();
			
			
			
			for (int i=0; i<n; i++) {
				int sum = 0;
				int a = sc.nextInt();
				for (int j=0; j<=a; j++) {
				if (j % 2 == 1) {
				sum += j;				
				}
				}
				System.out.println(sum);
				}
			
			
			
			}
		}
	}


