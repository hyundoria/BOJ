import java.util.Scanner;

public class Main {	 
	public static void main(String[] args)  {
		try (Scanner sc = new Scanner(System.in)) {
					
			int n = sc.nextInt();
			
			int sum = 0;
			
			for (int i = 0; i < 9; i++) {
				sum += sc.nextInt();
			}
			
			int value = n - sum;
			System.out.println(value);
			
		
		
		}		
	}
}

